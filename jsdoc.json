{
  "source": {
    "include": ["./src", "./index.js", "./README.md"]
  },
  "opts": {
    "destination": "/mnt/neuroverse/https-neurons/subDomains/docs/public/all.this",
    "template": "/mnt/neuroverse/suign.github.io/Sandbox/better-docs/", 
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
        "staticFiles": {
          "include": [
              "./README.md"
          ]
        }
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
      "parser": "gfm",
      "hardwrap": true
    },
    "better-docs": {
        "name": "All.This",
        "title": "All.This", 
        "css": "style.css",
        "trackingCode": "...",
  "hideGenerator": false,
  "navLinks": [
    {
      "label": "All.This",
      "href": "https://docs.neurons.me/all.this/"
    },
    {
      "label": "Neurons.me",
      "href": "https://neurons.me"
    },
    {
      "label": "Github",
      "href": "https://github.com/neurons-me/all.this"
    }
  ]
    }
}
