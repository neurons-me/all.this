{
  "source": {
    "include": ["./src", "./index.js", "./README.md"]
  },
  "opts": {
    "destination": "./docs/",
    "template": "../suign.github.io/dev_tools/Sandbox/better-docs/",    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
        "staticFiles": {
          "include": [
              "./README.md"
          ]
        }
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
      "parser": "gfm",
      "hardwrap": true
    },
    "better-docs": {
        "name": "All.This",
        "title": "All.This", 
        "css": "style.css",
        "trackingCode": "...",
  "hideGenerator": false,
  "navLinks": [
